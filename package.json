{
  "name": "ctrlcontas",
  "version": "1.0.0",
  "description": "API REST desenvolvida utilizando TypeScript",
  "main": "server/server.ts",
  "scripts": {
    "start": "set node_env=development && ts-node ./server/server.ts",
    "builded": "set node_env=production && node ./build/server/server.js",
    "unit-test": "set node_env=test && mocha --opts ./build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "set node_env=test && mocha --opts ./build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test"
  },
  "author": "tvSantiago",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parse": "^0.1.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "http-status": "^1.3.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.0",
    "sequelize": "^5.11.0",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.36",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.28.4",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "testdouble": "^3.12.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
